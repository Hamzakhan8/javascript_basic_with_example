{
    "manifest_version": 2,
    "name": "Change Currency Extension",
    "version": "1.0",
    "description": "Converts currency on the fly",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  